class String {
    int8* cStr;
    int32 size;

    String() = {
        this.cStr = "";
        this.size = 0;
    }

    String(int8* cStr) = {
        this.cStr = cStr;
        this.size = _StrLen(cStr);
    }

    void Print() = {
        _Print(this.cStr);
    }
}

let int32 Main() = {
    let String msg = new String("Hello World!");
    msg.Print();
}