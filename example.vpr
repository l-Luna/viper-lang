import void Print(int8* data);
import int32 _StrLen(int8* data);

struct String {
    int8* cStr;
    int32 size;
}

let String String(int8* cStr) = {
    let String str;
    str.cStr = cStr;
    str.size = _StrLen(cStr);

    return str;
}

let void Print(String str) = {
    Print(str.cStr);
}

let int32 Main() = {
    let String myStr = String("Hello World!\n");
    Print(myStr);
}